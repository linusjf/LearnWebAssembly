{
  "options": {
    "initialMemory": 1,
    "maximumMemory": 2,
    "memoryBase": 0
  },
  "targets": {
    "debug": {
      "binaryFile": "build/untouched.wasm",
      "debug": true,
      "outFile": "build/untouched.wasm",
      "sourceMap": true,
      "textFile": "build/untouched.wat"
    },
    "release": {
      "binaryFile": "build/optimized.wasm",
      "converge": false,
      "noAssert": false,
      "optimizeLevel": 3,
      "outFile": "build/optimized.wasm",
      "shrinkLevel": 1,
      "sourceMap": true,
      "textFile": "build/optimized.wat"
    }
  }
}
